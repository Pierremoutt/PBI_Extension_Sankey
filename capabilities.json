{
  "dataRoles": [
    {
      "displayName": "Category Data",
      "name": "category",
      "kind": "Grouping"
    },
    {
      "displayName": "Measure Data",
      "name": "measure",
      "kind": "Measure"
    }
  ],

  "dataViewMappings": [
    {
      "categorical": {
        "categories": {
          "for": {
            "in": "category"
          },
          "dataReductionAlgorithm": {
            "top": {}
          }
        },
        "values": {
          "select": [
            {
              "bind": {
                "to": "measure"
              }
            }
          ]
        }
      }
    }
  ],
  "privileges": [],
  "objects": {
    "enableAxis": {
      "properties": {
        "show": {
          "type": {
            "bool": true
          }
        },
        "fill": {
          "type": {
            "fill": {
              "solid": {
                "color": true
              }
            }
          }
        }
      }
    },
    "colorSelector": {
      "properties": {
        "linkColorSource": {
          "displayName": "Link Color Source",
          "type": {
            "enumeration": [{ "value": "source" }, { "value": "target" }]
          }
        }
      }
    }
  }
}
